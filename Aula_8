nano python6_2.py
!/usr/bin/env python3
import sys

seq_fqo = open("python_06_uc.txt", "w")
with open ("python_06.txt", "r") as seq_fh:
 for line in seq_fh:
  line = line.rstrip().upper()
  seq_fqo.write(f'{line}\n')

