brenda@brenda-Lenovo-IdeaPad-S145-15IIL:~$ chmod +x python6_1.py
brenda@brenda-Lenovo-IdeaPad-S145-15IIL:~$ ./python6_1.py
brenda@brenda-Lenovo-IdeaPad-S145-15IIL:~$ nano python6_1.py

python6_1.py:

#!/usr/bin/env python3 
import sys

#Tal exercício visa fazer um dicionário
genes = {}
with open("sequence_ex.txt","r") as seq_read:
  for line in seq_read:
    line = line.rstrip() 
    gene_id,seq = line.split() #split on whitespace
    genes[gene_id] = seq
print(genes)

